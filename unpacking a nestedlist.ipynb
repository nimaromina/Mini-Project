   "source": [
    "def flatten(nested_list):\n",
    "    lst = []\n",
    "    for item in nested_list:\n",
    "        if isinstance(item,list):\n",
    "            lst.extend(flatten(item))\n",
    "        else:\n",
    "            lst.append(item)\n",
    "    return lst"
   ]
